<section class="cut-2 section-wrap">
    <div class="section-container">
        <div class="cut-2-swiper">
            <div class="swiper">
                <div class="swiper-wrapper">
                    <div class="swiper-slide">
                        <div class="cut-2-slide slide-1">
                            <div class="cut-2-slide-img">
                                <picture>
                                    <source
                                        srcset="[[ IMG_PATH ]]cut-2-slide-1-m.webp"
                                        media="(max-width: 1024px)" />
                                    <img
                                        src="[[ IMG_PATH ]]cut-2-slide-1.webp"
                                        alt="" />
                                </picture>
                            </div>
                            <div class="cut-2-slide-content">
                                {% for i in range(1, 3) %}
                                <div class="txt-[[i]] svgtxt" data-swiper-parallax="-[[i]]00">
                                    <img
                                        class="svgtxt-img"
                                        src="[[ IMG_PATH ]]cut-2-slide-1-txt-[[i]].svg"
                                        alt="" />
                                </div>
                                {% endfor %}
                                <div class="cut-2-slide-content-line" data-swiper-parallax="-150"></div>
                                {% for i in range(3, 5) %}
                                <div class="txt-[[i]] svgtxt" data-swiper-parallax="-[[2*i]]00">
                                    <img
                                        class="svgtxt-img"
                                        src="[[ IMG_PATH ]]cut-2-slide-1-txt-[[i]].svg"
                                        alt="" />
                                </div>
                                {% endfor %}
                            </div>
                            <div class="cut-2-slide-pattern">
                                <img src="[[ IMG_PATH ]]cut-2-slide-1-pattern.svg" alt="" />
                            </div>
                        </div>
                    </div>
                    <div class="swiper-slide">
                        <div class="cut-2-slide slide-2">
                            <div class="cut-2-slide-img">
                                <picture>
                                    <source
                                        srcset="[[ IMG_PATH ]]cut-2-slide-2-m.webp"
                                        media="(max-width: 1024px)" />
                                    <img
                                        src="[[ IMG_PATH ]]cut-2-slide-2.webp"
                                        alt="" />
                                </picture>
                            </div>
                            <div class="cut-2-slide-content">
                                {% for i in range(1, 3) %}
                                <div class="txt-[[i]] svgtxt" data-swiper-parallax="-[[i]]00">
                                    <img
                                        class="svgtxt-img"
                                        src="[[ IMG_PATH ]]cut-2-slide-2-txt-[[i]].svg"
                                        alt="" />
                                </div>
                                {% endfor %}
                                <div class="cut-2-slide-content-line" data-swiper-parallax="-150"></div>
                                {% for i in range(3, 5) %}
                                <div class="txt-[[i]] svgtxt" data-swiper-parallax="-[[2*i]]00">
                                    <img
                                        class="svgtxt-img"
                                        src="[[ IMG_PATH ]]cut-2-slide-2-txt-[[i]].svg"
                                        alt="" />
                                </div>
                                {% endfor %}
                            </div>
                        </div>
                    </div>
                </div>
                <div class="swiper-button-prev"></div>
                <div class="swiper-button-next"></div>
            </div>
        </div>
    </div>
</section>
